transcript on
if {[file exists rtl_work]} {
	vdel -lib rtl_work -all
}
vlib rtl_work
vmap work rtl_work

vlog -vlog01compat -work work +incdir+D:/DOC/quartus {D:/DOC/quartus/sha256_w_mem.v}

vlog -vlog01compat -work work +incdir+D:/DOC/quartus {D:/DOC/quartus/tb_sha256_w_mem.v}

vsim -t 1ps -L altera_ver -L lpm_ver -L sgate_ver -L altera_mf_ver -L altera_lnsim_ver -L cycloneii_ver -L rtl_work -L work -voptargs="+acc"  tb_sha256_w_mem

add wave *
view structure
view signals
run -all
